<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.fiction">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:name="com.framework.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="AllowBackup">
        <activity android:name=".mian.widget.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".fiction.search.contents.widget.SearchContentsActivity" />
        <activity android:name=".fiction.search.detail.widget.SearchDetailActivity" />
        <activity android:name=".fiction.zw81.contents.widget.ZWContentsActivity" />
        <activity android:name=".fiction.zw81.detail.widget.ZWDetailActivity" />
        <activity android:name=".fiction.zw81.contents.widget.ZWHomeContentsActivity" />
        <activity android:name=".fiction.zw81.detail.widget.ZWHomeDetailActivity" />
    </application>

</manifest>